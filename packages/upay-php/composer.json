{
    "name": "upay/upay-php",
    "description": "SDK oficial da Upay para PHP - Integração fácil com a API de pagamentos",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Upay",
            "email": "suporte@upaybr.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Upay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upay\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
